apiVersion: apiextensions.k8s.io/v1
kind: CustomResourceDefinition
metadata:
  name: results.meleeman
spec:
  group: meleeman
  versions:
    - name: v1
      served: true
      storage: true
      schema:
        openAPIV3Schema:
          type: object
          properties:
            task:
              type: string
            result:
              type: string
            courses:
              type: array
              items:
                type: string

  scope: Namespaced
  names:
    plural: results
    singular: result
    kind: Task
    shortNames:
    - rs
